<h1>Dreams#index</h1>
<p>Find me in app/views/dreams/index.html.erb</p>

<% @dreams.each do |element| %>
<p><b>
  <%= element.author %>
  </b></p>
<p>
  <%= raw "<i>#{element.content}</i> tagged with <b>#{element.tags} </b>" %>
  </p>
  <p>
    <%= image_tag element.photo_url, size: "200x200" %>
  </p>
<p><%= link_to 'Destroy', dream_path(element), data: {confirm: 'Are you sure?'}, method: :delete %></p>
<% end %>